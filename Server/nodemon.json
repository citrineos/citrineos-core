{
  "watch": [
    "src",
    "../00_Base",
    "../01_Data",
    "../02_Util",
    "../03_Modules"
  ],
  "ext": ".ts,.js",
  "ignore": [],
  "exec": "npm run build --prefix ../ && node --inspect=0.0.0.0:9229 --nolazy -r ts-node/register",
  "events": {
    "crash": "nodemon --delay 500ms -L"
  }
}
